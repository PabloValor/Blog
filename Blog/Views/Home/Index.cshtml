@using Data
@model IList<Post>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>
        <h1>Hello world!</h1>
        <h2>I'm using IIS!</h2>
        @foreach (var post in Model)
        {
            <h3>Id: @post.id</h3>
            <h3>Título: @post.titulo</h3>
        }
    </div>
</body>
</html>
